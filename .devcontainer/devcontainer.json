{
    "name": "python-devcontainer-template",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "..",
        "args": {
            "VARIANT": "3.10.4-bullseye",
        }
    },
    "settings": {
        "terminal.integrated.profiles.linux": {
            "bash": {
                "path": "/bin/bash",
                "args": [
                    "-l"
                ]
            }
        }
    },
    "extensions": [
        "ms-python.python",
        "VisualStudioExptTeam.vscodeintellicode"
    ],
    "postCreateCommand": "../.devcontainer/postCrateCommand.sh",
}